<Window x:Class="Wayz.WOrientationEmailAccount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WOrientationEmailAccount" Height="500" Width="700" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image Margin="10,10,439,0" Source="exia-logo-20111.png" Stretch="Fill" Width="243" Height="94" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Label Content="AdminUsername" HorizontalAlignment="Left" Margin="572,10,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Button x:Name="BtnLogout" Content="Logout" HorizontalAlignment="Left" Margin="607,44,0,0" VerticalAlignment="Top" Width="75" Click="BtnLogout_Click"/>
        <Label Content="Email :" HorizontalAlignment="Center" Margin="128,241,478,188" VerticalAlignment="Center" FontSize="18" FontWeight="Bold" Height="41" Width="86"/>
        <Button x:Name="BtnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="597,429,0,0" VerticalAlignment="Top" Width="75" Click="BtnCancel_Click"/>
        <TextBox HorizontalAlignment="Center" Height="23" Margin="220,242,206.6,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="267"/>
        <Button x:Name="BtnSave" Content="Save" HorizontalAlignment="Left" Margin="492,429,0,0" VerticalAlignment="Top" Width="75" Click="BtnSave_Click"/>
    </Grid>
</Window>
