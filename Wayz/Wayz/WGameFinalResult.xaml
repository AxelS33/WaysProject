        <Button Content="Final Result" HorizontalAlignment="Left" Margin="600,0,0,0" VerticalAlignment="Bottom" Width="92"/>
        <Label Content="To complete this questionnary, click on Help us to learn more about you" HorizontalAlignment="Left" Margin="28,199,0,0" VerticalAlignment="Top"/>
        <Button Content="Help us to learn more about you" HorizontalAlignment="Center" Margin="240,277,239,0" VerticalAlignment="Top" Width="213"/>
