        <Button Content="Q1" HorizontalAlignment="Left" Margin="0,448,0,0" VerticalAlignment="Bottom" Width="60"/>
        <Button Content="Q2" HorizontalAlignment="Left" Margin="60,0,0,0" VerticalAlignment="Bottom" Width="60"/>
        <Button Content="Q3" HorizontalAlignment="Left" Margin="120,0,0,0" VerticalAlignment="Bottom" Width="60"/>
